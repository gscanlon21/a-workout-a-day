@inherits RefreshablePageBase
@using CommunityToolkit.Maui.Alerts

<Lib.AppMain Email='@Email' Token="@Token" Id="@Id" />

@code {
    [Parameter]
    public string Email { get; set; } = null!;

    [Parameter]
    public string Token { get; set; } = null!;

    [Parameter]
    public int? Id { get; set; }
}
